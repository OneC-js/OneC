    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome",
            "url": "http://localhost:9000",
            "webRoot": "${workspaceFolder}",
            "runtimeArgs": [
                "--incognito"
            ],
        },
        {
            "type": "firefox",
            "request": "launch",
            "name": "Firefox",
            "url": "http://localhost:9000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}